{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "moduleResolution": "node",
  },
  "esm": true,
  "typedocOptions": {
    "out": "docs/",
    "entryPointStrategy": "expand",
    "exclude": [
      "node_modules/**/*"
    ],
    "plugin": [
      "typedoc-theme-hierarchy",
      "typedoc-plugin-missing-exports",
      "@knodes/typedoc-plugin-pages"
    ],
    "pluginPages": {
      "pages": [ { "title": "NamelessBot", "moduleRoot": true, "children": [
        { "title": "Configuration", "source": "configuration.md" }
      ] } ],
      "source": "docs_pages"
    },
    "navigationLinks": {
      "Configuration": "pages/configuration.html"
    },
    "theme": "hierarchy",
    "readme": "README.md",
    "name": "NamelessBot",
  }
}
